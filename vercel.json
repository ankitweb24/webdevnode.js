{
    "version": 2,
    "builds": [
        {
            "src": "*.html",
            "use": "@vercel/static-build"
        },
        {
            "src": "*.js",
            "use": "@vercel/node-build"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "/index.js"
        }
    ]
}